.input-text {font-family: monospace;page-break-inside: avoid;
 white-space: pre-wrap;
}
.control-label{padding-top: 7pt;padding-bottom:7pt}
.inner-col{padding-right:7pt;padding-left:7pt}
.header{border-bottom:1pt solid #999;}
.footer{border-top:1pt solid #999;}
.strong{font-weight:bold}
.em{font-style:italic}
.clearfix{clear: both;} /* not tested */
hr.line {margin:0}
.hr-muted{color:#777;}
.hr-primary{color:#337ab7;}
.hr-success{color:#3c763d;}
.hr-info{color:#31708f;}
.hr-warning{color:#8a6d3b;}
.hr-danger{color:#a94442;}
code,pre {
  font-size:11pt;
}
